{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "Display": "Vite",
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": false,
    "jsx": "react-jsx",
    "lib": [
      "DOM.Iterable",
      "DOM",
      "ESNext"
    ],
    "module": "ESNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "types": [
      "vite/client"
    ],
    "allowSyntheticDefaultImports": true
  }
}